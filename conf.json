{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": [ 
            "./src/resources/users/user.memory.repository.js",
            "./src/resources/board/board.memory.repository.js",
            "./src/resources/task/task.memory.repository.js"
        ],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}